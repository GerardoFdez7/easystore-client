    "test-errors:conflicts": "tsx lib/errors/test-error-handlers.ts --conflicts",
    "test-errors:distribution": "tsx lib/errors/test-error-handlers.ts --distribution",
    "test-errors:interactive": "tsx lib/errors/test-error-handlers.ts --interactive",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:component": "cypress run --component",
    "cypress:run:e2e": "cypress run --e2e",
    "test:e2e": "cypress run --e2e --browser chrome",
    "test:component": "cypress run --component --browser chrome"
