{
  "name": "easystore-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "storybook": "storybook dev -p 6006",
    "gql": "env-cmd -f .env graphql-codegen --config codegen.ts",
    "prod": "env-cmd -f .env next build && next start",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker": "docker-compose up",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "build-storybook": "storybook build",
    "prepare": "husky install",
    "test-errors": "tsx server/errors/tests/test-error-handlers.ts",
